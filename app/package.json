{
  "name": "app",
  "version": "0.0.0",
  "private": true,
  "scripts": {
    "serve": "npx vite --port 8080",
    "serve:localnetwork": "npx vite --port 8080 --host",
    "lint": "npx eslint ./src/ --ext .vue,.js,.ts,.cjs,.mjs --fix --ignore-path .gitignore",
    "preview": "npx vite preview --port 8080",
    "typecheck": "npx vue-tsc --noEmit",
    "build": "npx vite build",
    "build:delete": "npx rimraf ./dist",
    "build:full": "npm run typecheck && npm run build:delete && npm run build"
  },
  "dependencies": {
    "fuse.js": "^7.0.0",
    "pinia": "^2.1.7",
    "pinia-plugin-persistedstate": "^3.2.0",
    "simpler-fetch": "^10.3.1",
    "ulid": "^2.3.0",
    "vue": "^3.3.6",
    "vue-router": "^4.2.5",
    "vuedraggable": "^4.1.0"
  },
  "devDependencies": {
    "@rushstack/eslint-patch": "^1.5.1",
    "@types/node": "^18.16.19",
    "@vitejs/plugin-basic-ssl": "^1.0.1",
    "@vitejs/plugin-vue": "^4.4.0",
    "@vue/eslint-config-prettier": "^8.0.0",
    "@vue/eslint-config-typescript": "^12.0.0",
    "@vue/tsconfig": "^0.4.0",
    "autoprefixer": "^10.4.16",
    "eslint": "^8.52.0",
    "eslint-plugin-vue": "^9.18.0",
    "postcss": "^8.4.31",
    "prettier": "^3.0.3",
    "prettier-plugin-tailwindcss": "^0.5.6",
    "tailwindcss": "^3.3.3",
    "typescript": "^5.2.2",
    "vite": "^4.5.0",
    "vite-plugin-pwa": "^0.16.6",
    "vue-tsc": "^1.8.20"
  }
}
